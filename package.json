{
  "name": "felix-pago-playwright-framework",
  "version": "1.0.0",
  "description": "Felix Pago Playwright Framework for Payment Tests",
  "main": "index.js",
  "scripts": {
    "test": "playwright test",
    "test:headed": "playwright test --headed",
    "test:debug": "playwright test --debug",
    "test:ui": "playwright test --ui",
    "report": "playwright show-report",
    "install-browsers": "playwright install",
    "setup-sdk": "./scripts/setup-typescript-sdk.sh",
    "setup-integration": "./scripts/setup-typescript-sdk.sh",
    "integration-test": "node scripts/integration-test.js",
    "integration-test:setup": "node scripts/setup-test-order.js",
    "integration-test:legacy": "node scripts/run-integration-test.js",
    "test:combined-chrome": "playwright test tests/combined-payment-scenarios.spec.ts --project=chromium",
    "cleanup-gcp": "./scripts/cleanup-gcp-credentials.sh",
    "fix-credentials": "./scripts/fix-credentials.sh",
    "clean-credentials": "./scripts/cleanup-credentials.sh",
    "cleanup-keys": "./scripts/cleanup-keys.sh",
    "create-order": "tsx scripts/create-test-order.ts",
    "create-order:js": "node dist/scripts/create-test-order.js",
    "setup": "./scripts/setup-npmrc.sh",
    "test:cash-payment": "node scripts/test-cash-payment.js test:cash-payment",
    "test:cash-payment:headed": "node scripts/test-cash-payment.js test:cash-payment:headed",
    "test:cash-payment:debug": "node scripts/test-cash-payment.js test:cash-payment:debug",
    "test:cash-payment:ui": "node scripts/test-cash-payment.js test:cash-payment:ui",
    "test:cash-payment:chrome": "node scripts/test-cash-payment.js test:cash-payment:chrome",
    "cash-payment-flow": "node scripts/cash-payment-integration.js",
    "cash-payment-flow:headed": "node scripts/cash-payment-integration.js && npm run test:cash-payment:headed",
    "cash-payment-flow:debug": "node scripts/cash-payment-integration.js && npm run test:cash-payment:debug",
    "cash-payment-test": "node scripts/cash-payment-test-hardcoded.js",
    "cash-payment-test:headed": "node scripts/cash-payment-test-hardcoded.js && npm run test:cash-payment:headed",
    "create-cash-order": "tsx scripts/create-cash-payment-order.ts",
    "update-cash-order": "node scripts/update-cash-payment-order.js"
  },
  "keywords": [
    "playwright",
    "testing",
    "automation",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.0.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "@bufbuild/protobuf": "^1.10.1",
    "@felix/overseer-sdk-ts": "^1.13.0",
    "@google-cloud/logging": "^11.1.0",
    "@google-cloud/pubsub": "^4.3.3",
    "@google-cloud/secret-manager": "^5.0.0",
    "@google-cloud/storage": "^7.7.0",
    "@googlemaps/js-api-loader": "^1.16.6",
    "@grpc/grpc-js": "^1.13.4",
    "@grpc/proto-loader": "^0.7.13",
    "google-auth-library": "^9.11.0",
    "jsonwebtoken": "^9.0.2",
    "node-cache": "^5.1.2",
    "protobufjs": "^7.4.0",
    "uuid": "^10.0.0"
  }
}
